{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "plain": { "format": "%(asctime)s %(levelname)s %(name)s - %(message)s" }
  },
  "handlers": {
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "filename": "/home/jiaheng/vllm_log/server.log",
      "maxBytes": 104857600,
      "backupCount": 5,
      "formatter": "plain",
      "level": "INFO"
    },
    "task_file": {
      "()": "browser_use_task_handler.BrowserUseTaskHandler",
      "log_directory": "/home/jiaheng/vllm_log/task_logs",
      "idle_timeout_minutes": 5,
      "level": "INFO",
      "formatter": "plain"
    },
    "uvicorn_file": {
      "class": "logging.handlers.RotatingFileHandler",
      "filename": "/home/jiaheng/vllm_log/uvicorn.log",
      "maxBytes": 104857600,
      "backupCount": 5,
      "formatter": "plain",
      "level": "INFO"
    }
  },
  "loggers": {
    "vllm": { "handlers": ["file", "task_file"], "level": "INFO", "propagate": false },
    "uvicorn": { "handlers": ["uvicorn_file"], "level": "INFO", "propagate": false },
    "uvicorn.access": { "handlers": ["uvicorn_file"], "level": "INFO", "propagate": false }
  }
}
